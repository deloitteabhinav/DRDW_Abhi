    <validationRules>
        <fullName>Anticipated_Date_Validation</fullName>
        <active>true</active>
        <description>Prevents Anticipated End date to be before Start Date</description>
        <errorConditionFormula>Anticipated_End_Date__c &lt;= Anticipated_Start_Date__c</errorConditionFormula>
        <errorDisplayField>Anticipated_End_Date__c</errorDisplayField>
        <errorMessage>Do you have a time machine? The Anticipated End Date cannot occur before the Anticipated Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IS_Validation</fullName>
        <active>true</active>
        <description>&quot;Does the Study/Project involve LY? &quot;field is required when Status= Active, Completed or Closed.</description>
        <errorConditionFormula>AND(OR(ISPICKVAL( Project_Status__c , &apos;Active&apos;) ,ISPICKVAL( Project_Status__c , &apos;Completed&apos;) ,ISPICKVAL( Project_Status__c , &apos;Closed&apos;)), ISPICKVAL( Is_Molecule__c,&apos;&apos;), TEXT(Research_Type__c)!=&apos;Research Topic&apos;,TEXT(Study_Description__c) != &apos;Modeling&apos;, TEXT(Study_Description__c) != &apos;Systematic Literature Review&apos;)</errorConditionFormula>
        <errorDisplayField>Is_Molecule__c</errorDisplayField>
        <errorMessage>This field is required when Status= Active, Completed or Closed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valid_Protocol_Approval</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Protocol_Approval_Required__c,&apos;Yes&apos;),ISBLANK( Protocol_Approval_Date__c ), Final_Protocol_checkbox__c ,NOT(ISNEW()))</errorConditionFormula>
        <errorDisplayField>Protocol_Approval_Date__c</errorDisplayField>
        <errorMessage>This field is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valid_Study_Approval</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( Study_Report_Approval_Required__c ,&apos;Yes&apos;),ISBLANK( Study_Report_Approval_Date__c ), Study_Report__c ,NOT(ISNEW()))</errorConditionFormula>
        <errorDisplayField>Study_Report_Approval_Date__c</errorDisplayField>
        <errorMessage>This field is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valid_VerifyStatus_ActualStartDate</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
AND(
TEXT(Research_Type__c)!=&apos;Research Topic&apos;,
NOT(ISPICKVAL(PRIORVALUE(Project_Status__c),&quot;Active&quot;)), 
NOT(ISPICKVAL(PRIORVALUE(Project_Status__c),&quot;Completed&quot;)),
NOT(ISPICKVAL(Project_Status__c,&quot;Active&quot;)),
NOT(ISPICKVAL(Project_Status__c,&quot;Completed&quot;)),
NOT(ISBLANK(Actual_Start_Date__c)),
ISCHANGED(Actual_Start_Date__c),
NOT(ISNEW())
),
AND(
TEXT(Research_Type__c)!=&apos;Research Topic&apos;,
NOT(ISPICKVAL(Project_Status__c,&quot;Active&quot;)),
NOT(ISPICKVAL(Project_Status__c,&quot;Completed&quot;)),
ISCHANGED(Actual_Start_Date__c),
NOT(ISBLANK(Actual_Start_Date__c)),
NOT(ISNEW())
)
)</errorConditionFormula>
        <errorDisplayField>Actual_Start_Date__c</errorDisplayField>
        <errorMessage>Oops, try again! Status must be Active or Completed to enter/update the Actual Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valid_Verify_ActualStartDate</fullName>
        <active>true</active>
        <errorConditionFormula>Actual_Start_Date__c &gt; TODAY()</errorConditionFormula>
        <errorDisplayField>Actual_Start_Date__c</errorDisplayField>
        <errorMessage>Do you have a time machine? The Actual Start Date cannot be a future date.</errorMessage>
    </validationRules>
