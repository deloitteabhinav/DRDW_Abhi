        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/apex.js&quot;)}

var userStoryId = &quot;{!TRAK_User_Story__c.Id}&quot;
var approvalStatus=&quot;{!TRAK_User_Story__c.Approval_Status__c}&quot;
var gxp=&quot;{!TRAK_User_Story__c.GXP__c}&quot;
var syscustodian=&quot;{!TRAK_User_Story__c.System_Custodian__c}&quot;
var productowner=&quot;{!TRAK_User_Story__c.System_Owner__c}&quot;
var compliance=&quot;{!TRAK_User_Story__c.Compliance_Owner__c}&quot;

if(syscustodian == &quot;&quot; || productowner == &quot;&quot;) 
alert(&apos;Please select a System Custodian and Product Owner before submitting this user story for approval. &apos;);
